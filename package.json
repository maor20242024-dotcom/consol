{
  "name": "nextjs_tailwind_shadcn_ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "echo \"⭐ Use this URL in Meta Webhooks: $NGROK_URL/api/webhooks/meta\" && next dev -p 3000",
    "test:webhook": "node scripts/test-meta-webhook.ts",
    "start:webhook": "node scripts/test-meta-webhook.ts",
    "setup:ngrok": "tsx scripts/setup-ngrok-webhook.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:validate": "prisma validate",
    "db:check": "tsx scripts/check-db.ts",
    "validate:routes": "[ $(find src/app -maxdepth 1 -type f -name 'page.tsx' | wc -l) -eq 0 ] && echo '✅ Routes OK' || echo '❌ Found page outside [locale]'",
    "validate:prisma": "prisma validate",
    "validate:i18n": "test -f src/messages/en.json && test -f src/messages/ar.json && echo '✅ Translations OK' || echo '❌ Missing translations'",
    "validate:build": "npx tsc --noEmit --skipLibCheck && npm run build",
    "validate:all": "npm run validate:routes && npm run validate:prisma && npm run validate:i18n && npm run lint && npm run build",
    "postinstall": "prisma generate"
  },
